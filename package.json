  "bin": {
    "crh": "./bin/crh"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "$(npm bin)/eslint .",
    "test": "npm run lint && npm run unit-test-coverage",
    "unit-test": "$(npm bin)/mocha -u bdd -R spec",
    "unit-test-coverage": "$(npm bin)/istanbul cover $(npm bin)/_mocha -- -u bdd -R spec --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/prosvita/crh.transliteration.git"
