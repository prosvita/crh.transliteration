    "Къырымтатар",
    "Кримськотатарська",
    "Крымскотатарский",
    "Крымско-татарский"
  ],
  "author": {
    "name": "Pavlo Bashynskyi",
    "email": "levonet@gmail.com",
    "url": "https://github.com/levonet"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prosvita/crh.transliteration/issues"
  },
  "homepage": "https://github.com/prosvita/crh.transliteration",
  "dependencies": {
    "commander": "^2.9.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.10.1",
    "eslint-config-pedant": "^0.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2"
  }
